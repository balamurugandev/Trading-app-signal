# Environment Configuration
NODE_ENV=development

# Live Data Configuration
LIVE_DATA=true

# Server Configuration
PORT=3001

# Market Data Settings
UPDATE_INTERVAL=5000
CACHE_TIMEOUT=5000

# Yahoo Finance Settings (no API key required)
YAHOO_TIMEOUT=10000
YAHOO_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Signal Generation Settings
SIGNAL_COOLDOWN=30000
MAX_SIGNALS_PER_MINUTE=5

# Demo Mode Settings (when LIVE_DATA=false)
DEMO_VOLATILITY=0.02
DEMO_UPDATE_INTERVAL=1000